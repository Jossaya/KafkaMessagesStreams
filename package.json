{
  "name": "kudobuzzchallenge-expressjs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "client": "cd kudobuzzchallenge_react && npm start",
    "server": "nodemon app.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "dev:server": "cd kudobuzzchallenge_react && npm run start && cd .. && npm run start",
    "start": "node app.js",
    "debug": "node --nolazy --inspect-brk=9229 mondodbWriter.js"
  },
  "dependencies": {
    "concurrently": "^4.1.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "kafka-node": "^3.0.1",
    "mongodb": "^3.1.10",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11"
  }
}
